{
	"name": "joker-search-editor-apply-changes",
	"displayName": "JOKER Search Editor",
	"description": "JOKER Apply local Search Editor edits",
	"publisher": "akwigliz",
	"repository": {
		"url": "https://github.com/JacksonKearl/vscode-search-editor-apply-changes"
	},
	"icon": "icon.png",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:searchEditorApplyChanges.apply"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "searchEditorApplyChanges.apply",
				"enablement": "editorLangId == search-result",
				"title": "Apply Search Editor changes to workspace"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^5.4.5",
		"vscode-test": "^1.2.2"
	}
}
